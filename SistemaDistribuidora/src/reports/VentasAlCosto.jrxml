	<parameter name="fecha" class="java.util.Date"/>
	<parameter name="vendedores" class="java.util.Collection" nestedType="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT ventasfinal.fecha,
	vendedores.nombre,
	ventasfinal.codart AS CodArt,
	ventasfinal.descripcion AS Descripcion,
	sum( ventasfinal.cantidad) AS Cantidad,
	articulos.costo AS CostoU,
	sum( ventasfinal.subtotalcosto) AS SubtotalCosto
FROM ventasfinal
	inner join articulos ON 
	 ventasfinal.codart = articulos.codigo
	inner join vendedores ON 
	 vendedores.cod_vendedor = ventasfinal.cod_vendedor 
WHERE ventasfinal.fecha = $P{fecha} and  $X{IN, vendedores.cod_vendedor, vendedores}  and ventasfinal.ganancia = 0
group by ventasfinal.fecha, vendedores.nombre, ventasfinal.codart, ventasfinal.descripcion, articulos.costo]]>
	</queryString>
